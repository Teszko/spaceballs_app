#!/usr/bin/env node

var electron = require('electron-prebuilt')

var proc = require('child_process')

var args = process.argv.slice( 2 ) ;  
args.unshift( __dirname + '/../' ) ;

proc.spawn( electron , args , { stdio: 'inherit' } ) ;


